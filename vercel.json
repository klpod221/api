{
    "version": 2,
    "name": "api.klpod221",
    "routes": [
        { "src": "/(.*)", "dest": "/index.js" }
    ],
    "builds": [
        { 
            "src": "index.js", 
            "use": "@vercel/node"
        }
    ],
    "crons": [
        {
            "path": "./worker.js",
            "schedule": "0 0 * * *"
        }
    ]
}
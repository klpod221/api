{
    "version": 2,
    "name": "api.klpod221",
    "builds": [
        { "src": "*.js", "use": "@vercel/node" },
        { "src": "public/**", "use": "@vercel/static" },
        { "src": "views/**", "use": "@vercel/static" }
    ],
    "trailingSlash": true,
    "routes": [
        { "src": "/(.*)", "dest": "/index.js" }
    ]
}
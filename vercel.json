{
    "version": 2,
    "name": "api.klpod221",
    "builds": [
        { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/index.js" }
    ],
    "crons": [
        {
            "name": "api-worker",
            "command": "node worker.js",
            "schedule": "0 0 * * *"
        }
    ]
}